<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.s205452lykkehjulet.ui.GameFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_game" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.s205452lykkehjulet.ui.HighscoreFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_highscore" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.s205452lykkehjulet.ui.EndMessageFragment"
        android:label="@string/title_notifications"
